{
  "version": 2,
  "name": "review-copier-frontend",
  "env": {
    "NODE_OPTIONS": "--openssl-legacy-provider"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://backend-npe9crcnd-manoj-mallas-projects.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
